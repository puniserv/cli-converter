{
  "name": "puniserv/cli-converter",
  "description": "PHP CLI converter",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Src\\": "src",
      "test\\": "test"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "7.5.9"
  },
  "require": {
    "php": "^7.1",
    "ext-simplexml": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "symfony/console": "4.2.8",
    "symfony/process": "4.2.8",
    "guzzlehttp/guzzle": "6.3.3",
    "symfony/dependency-injection": "4.2.8",
    "symfony/config": "4.2.8"
  },
  "scripts": {
    "test": "phpunit test",
    "phpunit": "phpunit test",
    "testCoverageWithHtml": "phpunit test --coverage-html coverage --whitelist src",
    "testCoverage": "phpunit test --coverage-text --whitelist src"
  }
}
